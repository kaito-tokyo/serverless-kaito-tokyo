{
  "$schema": "../../node_modules/wrangler/config-schema.json",
  "name": "serverless-kaito-tokyo-web",
  "compatibility_date": "2025-05-05",
  "main": "./api/src/index.ts",
  "assets": {
    "directory": "./dist",
    "binding": "ASSETS",
    "run_worker_first": ["/api/*", "/internal/*"],
  },
  "ai": {
    "binding": "AI",
  },
  "observability": {
    "enabled": true,
  },
  "preview_urls": true,
}
